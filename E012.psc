Algoritmo CUENTA 
	definir c,j,a,pc,pj,pa,mt Como Real; 
	escribir "Plato de carta"
	leer c
	escribir "jarras chicas"
	leer j
	escribir "aperitivo"
	Leer a
	pc<-c*25
	pj<-j*8
	pa<-a*10
	mt<-pc+pj+pa
	escribir"el precio a pagar es:",mt;
	Escribir"platos de entrada comprados",c;
	Escribir"jarras chicas compradas",j;
	Escribir"aperitivos comprados",a;
FinAlgoritmo
