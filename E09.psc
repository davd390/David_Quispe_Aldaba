Algoritmo pollo 
	definir p,g,e,pp,pg,pe,mt como real; 
	escribir "pollos comprados"
	leer p
	escribir "gaseosa comprados"
	leer g 
	escribir "ensalada comprados"
	Leer v
	escribir "vino comprados"
	Leer e
	pp<-p*24
	pg<-g*7.5
	pv<-v*35
	pe<-e*5
	mt<-pp+pg+pv+pe
	escribir"el precio a pagar es:",mt;
	Escribir"pollos comprados",p;
	Escribir"gaseosas compradas",g;
	Escribir"vinos comprados",v;
	Escribir"ensaladas compradas",e;
FinAlgoritmo

